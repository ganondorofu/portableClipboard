    "]": ["BACKSLASH"],
    ":": ["QUOTE"],
    ";": ["SEMICOLON"],
    "^": ["EQUALS"]
  },
  "shift_symbols": {
    "\"": ["SHIFT", "TWO"],
    "&": ["SHIFT", "SIX"],
    "'": ["SHIFT", "SEVEN"],
    "(": ["SHIFT", "EIGHT"],
    ")": ["SHIFT", "NINE"],
    "=": ["SHIFT", "MINUS"],
